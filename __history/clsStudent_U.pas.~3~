unit clsStudent_U;

interface

uses clsTutor_u;

type
  TStudent = class(TTutor)
  private
    fGender: char;
    fAttendedSessions: integer;

  public
    constructor Create(sUsername, sPassword, sFirstname, sSurname: string);
      overload;
    constructor Create(sUsername, sPassword, sFirstname, sSurname: string;
      cGender:char;iAttendedSessions: integer); overload;
  end;

implementation

end.
